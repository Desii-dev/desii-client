{
  "name": "desii-client",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "test": "jest",
    "snapshot-update": "jest --updateSnapshot",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook",
    "gen-types": "cross-env DOTENV_CONFIG_PATH=./.env.local graphql-codegen --config codegen.yml -r dotenv/config",
    "studio": "prisma studio",
    "vercel-build": "prisma generate && prisma migrate deploy && next build"
  },
  "dependencies": {
    "@apollo/client": "3.5.9",
    "@apollo/react-hooks": "4.0.0",
    "@chakra-ui/icons": "1.1.5",
    "@chakra-ui/react": "1.7.5",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@next-auth/prisma-adapter": "1.0.1",
    "@prisma/client": "3.8.1",
    "apollo-server-micro": "3.6.3",
    "cross-fetch": "3.1.5",
    "firebase": "9.6.5",
    "framer-motion": "4.1.17",
    "graphql": "15.7.2",
    "graphql-scalars": "1.14.1",
    "micro": "9.3.4",
    "micro-cors": "0.1.1",
    "next": "12.0.10",
    "nexus": "1.1.0",
    "nookies": "2.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.16.12",
    "@graphql-codegen/cli": "2.4.0",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-graphql-request": "4.3.3",
    "@graphql-codegen/typescript-operations": "2.2.2",
    "@graphql-codegen/typescript-react-apollo": "3.2.4",
    "@graphql-codegen/typescript-resolvers": "2.4.3",
    "@storybook/addon-actions": "6.4.14",
    "@storybook/addon-essentials": "6.4.14",
    "@storybook/addon-links": "6.4.14",
    "@storybook/react": "6.4.14",
    "@testing-library/react": "12.1.3",
    "@types/jest": "27.4.0",
    "@types/micro-cors": "0.1.2",
    "@types/next-auth": "3.15.0",
    "@types/node": "16.11.25",
    "@types/react": "17.0.39",
    "babel-jest": "27.4.6",
    "babel-loader": "8.2.3",
    "cross-env": "7.0.3",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-plugin-storybook": "0.5.7",
    "jest": "27.4.7",
    "msw": "0.36.8",
    "msw-storybook-addon": "1.6.0",
    "next-auth": "4.1.2",
    "prettier": "2.5.1",
    "prisma": "3.8.1",
    "ts-jest": "27.1.3",
    "ts-node": "10.4.0",
    "typescript": "4.5.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
